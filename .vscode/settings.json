{
  "search.exclude": {
    "**/.sst": true
  },
  "cSpell.words": [
    "automatutor",
    "dottified",
    "edusoftware",
    "leaderboard",
    "logictutor",
    "nanoid",
    "oidc",
    "tokenset",
    "unmarshall"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "svg.preview.background": "custom",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
